{
    "name": "codedsultan/modular-scaffolder",
    "description": "Laravel Modular CRUD Generator with Inertia + React + TypeScript support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ibraheem Olusegun",
            "email": "codedsultan369@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },

    "require-dev": {
        "orchestra/testbench": "^8.0"
    },

    "autoload": {
        "psr-4": {
            "Codedsultan\\ModularScaffolder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedsultan\\ModularScaffolder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedsultan\\ModularScaffolder\\ModularScaffolderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
